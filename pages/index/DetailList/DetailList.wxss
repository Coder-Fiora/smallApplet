/* pages/SpaList/SpaList.wxss */
.list{
    margin-bottom: 90rpx;
}
.list >image{
   width: 100%;
   height: 350rpx;
}
.list .title{
    display: flex;
    justify-content: space-between;
    padding: 0 60rpx;
    font-size: 32rpx;
    font-weight: 600;
    margin-top: 20rpx;
}
.list .title text{
    display: block;
    font-size: 22rpx;
    border: 1px solid #e6e6e6;
    border-radius: 8rpx;
    padding: 6rpx 14rpx;
    font-weight: 400;
    color: #999999;
}
.list .content{
    font-size: 28rpx;
    color: #333333;
    padding: 0 60rpx;
    margin-top: 20rpx;
}