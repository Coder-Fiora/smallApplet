/* pages/mail/mycar/mycar.wxss */
.maneger{
    display: flex;
    justify-content: space-between;
    padding: 36rpx 30rpx;
    border-bottom: 16rpx solid #ededed;
    font-weight: 500;
    font-size: 30rpx;
    letter-spacing: 2rpx;
}
.maneger view{
    font-size: 26rpx;
    color: #666666;
}
.head{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 30rpx 20rpx;
    padding-bottom: 10rpx;
}
.head >view:nth-of-type(1){
    width: 10%;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-right: 10rpx;
    margin-top: 50rpx;
}
.head >view:nth-of-type(2){
    flex: 1;
    padding-top: 30rpx;
    border-top: 1px solid #f6f6f6;
}
.head:nth-of-type(1) >view:nth-of-type(2){
    padding-top: 0;
    border: none;
}
.home{
    font-size: 24rpx;
    display: flex;
    align-items: center;
    margin-bottom: 36rpx;
}
.home image{
    width: 30rpx;
    height: 30rpx;
    margin-right: 10rpx;
}
.content_price{
    display: flex;
}
.content_price .left_image{
    width: 160rpx;
    height: 160rpx;
    margin-right: 20rpx;
    border-radius: 12rpx;
    overflow: hidden;
}
.content_price .left_image image{
    width: 100%;
    height: 100%;
}
.content_price .content{
    flex: 1;
}
.content_price .content .title{
    font-size: 28rpx;
    line-height: 54rpx;
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
    text-overflow: ellipsis;
}
.content_price .content .price_box{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.content_price .content .price_box >text{
    color:rgba(255,0,0);
    font-family: cursive;
}
.price_box .choose_num{
    display: flex;
    align-items: center;
    font-size: 24rpx;
}
.price_box .choose_num  .picker{
    padding: 8rpx 30rpx;
    background-color: #f6f6f6;
    margin: 0 10rpx;
}
.price_box .choose_num  >view image{
    width: 30rpx;
    height: 30rpx;
}
checkbox .wx-checkbox-input {
    width: 30rpx;
    height: 30rpx;
    border-radius: 50%;
  }
  
  /*checkbox选中后样式  */
  checkbox .wx-checkbox-input.wx-checkbox-input-checked {
    background: #dd3113;
  }
  
  /*checkbox选中后图标样式  */
  checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
    width: 30rpx;
    height: 30rpx;
    line-height: 28rpx;
    text-align: center;
    font-size: 18rpx;
    color: #fff;
    background: transparent;
    transform: translate(-50%, -50%) scale(1);
    -webkit-transform: translate(-50%, -50%) scale(1);
  }

  .compute{
      position: fixed;
      bottom: 0;
      width: 100%;
      padding: 0 20rpx;
      box-sizing: border-box;
      height: 100rpx;
      display: flex;
      align-items: center;
      border-top: 1px solid #f6f6f6;
      font-size: 24rpx;
  }
