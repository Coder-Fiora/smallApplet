.noLogin {
    background-color: #fdeff0;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 25rpx 40rpx;
    font-size: 24rpx;
    width: 100%;
    box-sizing: border-box;
    letter-spacing: 2rpx;
  }
  .noLogin view{
      width: 104rpx;
      height: 46rpx;
      border: 1px solid #f7cccc;
      color: #f53e3e;
      text-align: center;
      line-height: 46rpx;
  }
.bgbox {
  height: 460rpx;
  position: relative;
}
.bgbox::after{
    content: '';
    display: block;
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100rpx;
    background: linear-gradient(rgba(255,255,255,0),rgba(255,255,255,1));
    z-index: 6;
}
.bg1 {
  width: 100%;
  height: 100%;
  background-size: 100% 300rpx;
  background-position: 0 0;
  background-repeat: no-repeat;
  position: absolute;
  z-index: 10;
}
.bg2 {
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 5;
}
.centerCard {
  padding: 40rpx 45rpx;
  font-size: 28rpx;
  position: relative;
  margin: -100rpx 40rpx 0 40rpx;
  z-index: 8;
  border-radius: 8rpx;
  background: #ffffff;
  box-shadow: 0 0 10rpx #e6e6e6;
}
.centerCard .map {
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #f71b31;
  padding: 20rpx 0 30rpx 0;
  border-bottom: 1px solid #f6f6f6;
}
.centerCard .map view:nth-of-type(1) {
  color: #090909;
  margin-right: 30rpx;
}
.centerCard .map view {
  display: flex;
  align-items: center;
  font-weight: 600;
  letter-spacing: 2rpx;
}
.centerCard .map view .icon {
  display: block;
  width: 30rpx;
  height: 30rpx;
  margin-right: 5rpx;
}
.centerCard .date {
  display: flex;
  padding: 34rpx 0;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #f6f6f6;
}
.centerCard .date .startTime view:nth-of-type(2),
.centerCard .date .endTime view:nth-of-type(2) {
  font-weight: 500;
  font-size: 30rpx;
}
.centerCard .date .startTime view:nth-of-type(1),
.centerCard .date .endTime view:nth-of-type(1) {
  font-size: 24rpx;
  color: #afafaf;
}
.centerCard .date .endTime view{
    text-align: right;
}
.centerCard .date .tag {
  border: 1px solid #f59da1a6;
  padding: 4rpx 18rpx;
  color: #f33640;
  border-radius: 20rpx;
  font-size: 20rpx;
}
.centerCard .roomInfo {
  display: flex;
  justify-content: space-between;
  padding: 38rpx 0;
  border-bottom: 1px solid #f6f6f6;
  margin-bottom: 70rpx;
  font-weight: 500;
}
.centerCard .code {
  display: flex;
  padding: 30rpx 0;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
}
.centerCard .code > view view:nth-of-type(2) {
  color: #a2a2a2;
  font-size: 20rpx;
  text-decoration: underline;
  margin-top: 10rpx;
  letter-spacing: 2rpx;
}
.centerCard .code input {
  font-size: 24rpx;
  text-align: right;
}
.hotRecommend {
  padding: 30rpx 0;
  margin-top: 20rpx;
}
.hotRecommend .title {
  text-align: center;
  margin-bottom: 40rpx;
}
.hotRecommend .cards {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 0 50rpx;
}
.roomConfig {
  padding: 28rpx 50rpx;
}
.roomConfig .title {
  text-align: center;
  margin-bottom: 20rpx;
}
.roomConfig .config {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 0;
}
.roomConfig .config + .config {
  border-top: 1px solid #ececec;
}
.roomConfig .config:nth-last-of-type(1) {
  margin-bottom: 20rpx;
}
.roomConfig .add {
  background: #e51f2b;
  color: #efd9d8;
}
.search_btn{
    width: 100%;
    height: 90rpx;
    border-radius: 45rpx;
    background-color: #f11818;
    color: white;
    font-size: 32rpx;
    text-align: center;
    line-height: 90rpx;
    box-shadow: 0 0 4rpx 5rpx #f70d0d38;
}