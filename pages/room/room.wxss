.noLogin {
  background-color: #fef2f2;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 32rpx;
  font-size: 26rpx;
}
.bgbox {
  height: 400rpx;
  position: relative;
}
.bg1 {
  width: 100%;
  height: 100%;
  background-size: 100% 300rpx;
  background-position: 0 0;
  background-repeat: no-repeat;
  position: absolute;
  z-index: 10;
}
.bg2 {
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 5;
}
.centerCard {
  padding: 40rpx;
  font-size: 28rpx;
  position: relative;
  margin: -100rpx 50rpx 0 50rpx;
  z-index: 5;
  border-radius: 8rpx;
  background: #ffffff;
  box-shadow: 0 0 10rpx #e6e6e6;
}
.centerCard .map {
  display: flex;
  align-items: center;
  color: #cf3443;
  padding-bottom: 20rpx;
  border-bottom: 1px solid #ecebeb;
}
.centerCard .map view:nth-of-type(1) {
  color: #090909;
  margin-right: 20rpx;
}
.centerCard .map view {
  display: flex;
  align-items: center;
}
.centerCard .map view .icon {
  display: block;
  width: 30rpx;
  height: 30rpx;
  margin-right: 5rpx;
}
.centerCard .date {
  display: flex;
  padding: 20rpx 0;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #ecebeb;
}
.centerCard .date .startTime view:nth-of-type(1),
.centerCard .date .endTime view:nth-of-type(1) {
  font-size: 20rpx;
  color: #afafaf;
}
.centerCard .date .tag {
  border: 1px solid #d05057;
  padding: 2rpx 8rpx;
  color: #d05057;
  border-radius: 20rpx;
  font-size: 20rpx;
}
.centerCard .roomInfo {
  display: flex;
  justify-content: space-between;
  padding: 38rpx 0;
  border-bottom: 1px solid #ecebeb;
}
.centerCard .code {
  display: flex;
  padding: 30rpx 0;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
}
.centerCard .code > view view:nth-of-type(2) {
  color: #a2a2a2;
  font-size: 20rpx;
  text-decoration: underline;
  margin-top: 10rpx;
  letter-spacing: 2rpx;
}
.centerCard .code input {
  font-size: 24rpx;
  text-align: right;
}
.hotRecommend {
  padding: 30rpx 0;
  margin-top: 20rpx;
}
.hotRecommend .title {
  text-align: center;
  margin-bottom: 40rpx;
}
.hotRecommend .cards {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 0 50rpx;
}
.roomConfig {
  padding: 28rpx 50rpx;
}
.roomConfig .title {
  text-align: center;
  margin-bottom: 20rpx;
}
.roomConfig .config {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 0;
}
.roomConfig .config + .config {
  border-top: 1px solid #ececec;
}
.roomConfig .config:nth-last-of-type(1) {
  margin-bottom: 20rpx;
}
.roomConfig .add {
  background: #e51f2b;
  color: #efd9d8;
}
